5
f = int(input('секунды '))
c = 3600
m = 60
f1 = 60 
cc = f//c 
mm = f//m 
ff1 = f 
print(str(cc) + ' часы')
print(str(mm) + ' минуты')
print(str(ff1) + ' секунды')

6
k = int(input('введите номер квартиры '))  
p = 3 # кол-во подьездов 
ai = 9 # кол-во этажей
kai = 4 # кол-во квартир на этаже 
c = ai * kai  # кол-во квартир в подъезде
x = k // c
y = (x + 1)  # узнаём в каком подъезде квартира
z = k % c
d = z // kai
s = (d + 1)  # узнаём на каком этаже
print(str(y) + ' подъезд')
print(str(s) + ' этаж')


