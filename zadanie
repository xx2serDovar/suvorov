7
A = int(input('KOPEIKA '))
pyb = 100
gr = 10
alt = 3
pol = 0.25
pybl = A//pyb
gri = A//gr
alti = A//alt 
polyc = A//pol
print(str(pybl) + ' PYBLI ETO ')
print(str(gri) + ' ETO GRIVNA ')
print(str(alti) + ' ETO ALTIN ')
print(str(polyc) + ' ETO POLYCHKA ')



n1 = int(input('номер 1 столба  '))
n = int(input('номер столба '))
x = int(input('растояние до близ. столба '))
y = int(input('растоянием между столбами '))
x1 = x + y
while n < n1:
    k = x1 + y
    
    

print(str(n) + ' находится n столб ')

